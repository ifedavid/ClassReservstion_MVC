
@inject UserManager<ApplicationUser> UserManager
@{
    ViewData["Title"] = "MemoryCache";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



@model DateTime?
<div class="row">
    <div class="col-md-3">
        <h3> Current Time: @DateTime.Now.ToString()</h3>
    </div>
    <div class="col-md-3">
        <h3>Cached Time: @Model.Value.ToString()</h3>
    </div>
</div>